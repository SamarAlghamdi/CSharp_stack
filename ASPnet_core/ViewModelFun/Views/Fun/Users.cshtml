@using ViewModelFun.Models;
@model List<User>
<div class="contaner d-flex justify-content-center ">
<div class="d-flex justify-content-center flex-column w-20">
    <h1 class="p-2">Here are some Users!</h1>
    <div class="card p-2">
        @foreach(var user in Model){
            <p>@user.FirstName @user.LastName</p>
        }
    </div>
</div>
</div>